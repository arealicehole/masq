# ===========================================
# Image Processing Service Configuration
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# ----- Required API Keys -----
RUNWARE_API_KEY=your_runware_api_key_here
KIE_API_KEY=your_kie_api_key_here

# ----- Server Settings -----
LOG_LEVEL=INFO
MAX_CONCURRENT_JOBS=4
REQUEST_TIMEOUT=60

# ----- Storage -----
TEMP_DIR=/tmp/img-proc
CLEANUP_AFTER_SECONDS=300
MAX_IMAGE_SIZE_MB=50

# ----- Runware Settings -----
RUNWARE_TIMEOUT=120
RUNWARE_MAX_RETRIES=3

# ----- Kie.ai Settings -----
KIE_TIMEOUT=120
KIE_POLL_INTERVAL=2

# ----- Local Model Settings -----
REMBG_MODEL_DIR=~/.u2net
BIREFNET_MODEL=birefnet-general
ISNET_MODEL=isnet-general-use

# ----- Performance -----
UVICORN_WORKERS=2
PILLOW_SIMD=true
THREAD_POOL_SIZE=4

# ----- Phase 2 (GPU) -----
# USE_GPU=false
# COMPUTE_BACKEND=cpu
